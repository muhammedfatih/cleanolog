{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Colourful Log",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "comment.colourful-log",
				"match": "(\\[[0-9|m|;]*)"
			}]
		}
	},
	"scopeName": "source.colourful-log-viewer"
}